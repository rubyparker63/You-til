        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.499" />

    <Button
        android:id="@+id/button_0"
        android:layout_width="92dp"
        android:layout_height="54dp"
        android:background="@color/buttonAccent1"
